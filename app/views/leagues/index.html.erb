<h1>Leagues</h1>

<table class="table table-striped table-hover">
  <tr>
    <th>Name</th>
    <th>ID</th>
    <th>League URL</th>
  </tr>
  <% @leagues.each do |l| %>
    <tr>
      <td><%= l.name %></td>
      <td><%= l.id %></td>
      <td><%= link_to "#{l.url}", l.url%></td>
    </tr>
  <% end %>
